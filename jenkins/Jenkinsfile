pipeline{
    //agent any
    agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
        //docker { image 'node:16-alpine' }
    }
    stages{
        stage('build')
        {
            steps{
                sh 'mvn clean package'
            }
            post{
                success{
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }

        
    }
}